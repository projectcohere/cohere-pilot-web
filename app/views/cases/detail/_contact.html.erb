<%= section_tag(t(".title"), class: "ShowCase-contact") do %>
  <%= field_tag(t(".phone-number"), class: "ShowCase-phoneNumber") do %>
    <%= view.phone_number %>
  <%- end %>

  <%= field_tag(t(".address"), class: "ShowCase-address") do %>
    <% view.address.each do |line| %>
      <%= line %>
      <br>
    <% end %>
  <%- end %>
<%- end %>
